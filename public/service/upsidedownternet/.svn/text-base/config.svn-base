##
## Configuration for public/service/upsidedownternet
##

[apt]
install += ["squid", "apache2", "imagemagick", "dsniff"]

[files]
permission["/var/www/images"] = "0755"

owner["/var/www/images"] = "www-data:www-data"

[unixuser.www-data]
groups_include += "proxy"

[files]
mkdir += "/var/www/images"
