##
## Configuration for public/config/debian-multimedia
##

[apt.source.debian-multimedia]
url = "http://www.deb-multimedia.org"
components = ["main", "non-free"]
dist = "stable"
#keys += ""


[settings]
if PLATFORM_OS_NAME == "debian"
  if PLATFORM_OS_VERSION > "6.0"
    debian_distro = "squeeze"
  elif PLATFORM_OS_VERSION > "5.0"
    debian_distro = "lenny"
  else
    print_fatal("Debian version too old or new")
  end
else
  print_error("Please add support for non-debian distros in config/debian-multimedia")
end

