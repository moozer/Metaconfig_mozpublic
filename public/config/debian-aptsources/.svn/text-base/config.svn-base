##
## Configuration for public/config/debian-aptsources
##

[settings]
if PLATFORM_OS_NAME == "debian"
  if PLATFORM_OS_VERSION > "6.0"
    debian_distro = "squeeze"
  elif PLATFORM_OS_VERSION > "5.0"
    debian_distro = "lenny"
  else
    print_fatal("Debian version too old or new")
  end
elif PLATFORM_OS_NAME == "ubuntu"
  debian_distro = PLATFORM_OS_NICKNAME
else
  print_error("Please add support for non-debian/ubuntu distros in config/aptsources")
end

debian_components = ["main"]
mirror = "http://mirrors.telianet.dk"
updates = False
