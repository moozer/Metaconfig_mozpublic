[settings]
host = "127.0.0.1"
port = "8080"
server = "Metaconfig varnish profile"

ext_binary = ["jpg", "jpeg", "png", "gif", "gz", "tgz", "bz2", "mp3", "ogg", "ico", "rar", "zip", "7z"]
ext_remove_query = []
ext_ignore_cookie = ext_binary + ["pdf"]
ext_static = ext_ignore_cookie + []
ext_dynamic = ["php", "php3", "php4", "php5"]

url_pass = []
url_pipe = []
url_static = []
url_dynamic = []

ttl_default = "20s"
ttl_static = "30m"
ttl_dynamic = "60s"
ttl_notfound = "20s"
